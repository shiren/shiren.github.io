  },
  "description": "Gatsby Blog of Sungho Kim(shiren)",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "deploy": "gatsby clean && gatsby build && git checkout gh-pages && rm -rf ./docs && mv ./public ./docs && git add . && git commit -m \"Updated: `date +'%Y-%m-%d %H:%M:%S'`\" && git push && git checkout v2",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "browserslist_": [
    "last 2 version",
    "not ie <= 10"
  ],
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "disqus-react": "^1.0.10",
