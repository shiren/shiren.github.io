{
  "name": "shiren-gatsby-blog",
  "private": true,
  "author": {
    "name": "Sungho Kim",
    "email": "shirenbeat@gmail.com",
    "url": "https://shiren.dev"
  },
  "description": "Gatsby Blog of Sungho Kim(shiren)",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true && gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "deploy": "gatsby clean && gatsby build && git checkout gh-pages && cp -R ./public ./docs && git add . && git commit -m \"Updated: `date +'%Y-%m-%d %H:%M:%S'`\" && git push && git checkout v2 && rm -rf ./docs",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "browserslist_": [
    "last 2 version",
    "not ie <= 10"
  ],
  "dependencies": {
    "@isamrish/gatsby-plugin-google-adsense": "^1.2.0",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.24.32",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sitemap": "^2.4.13",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.16",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-remark": "^2.8.28",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-share": "^4.2.1",
    "styled-components": "^5.1.1",
    "styled-icons": "^10.15.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.2",
    "@types/webpack": "^4.41.21",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-tui": "^3.1.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
