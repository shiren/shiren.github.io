{
  "name": "shiren-gatsby-blog",
  "private": true,
  "author": {
    "name": "Sungho Kim",
    "email": "shirenbeat@gmail.com",
    "url": "https://shiren.dev"
  },
  "description": "Gatsby Blog of Sungho Kim(shiren)",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "deploy": "gatsby clean && gatsby build && git checkout gh-pages && cp -R ./public ./docs && git add . && git commit -m \"Updated: `date +'%Y-%m-%d %H:%M:%S'`\" && git push && git checkout v2 && rm -rf ./docs",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "browserslist_": [
    "last 2 version",
    "not ie <= 10"
  ],
  "dependencies": {
    "@isamrish/gatsby-plugin-google-adsense": "^1.2.0",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.0.2",
    "gatsby-plugin-feed": "^3.0.0",
    "gatsby-plugin-google-analytics": "^3.0.0",
    "gatsby-plugin-google-gtag": "^3.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-plugin-styled-components": "^4.0.0",
    "gatsby-plugin-typescript": "^3.0.0",
    "gatsby-remark-prismjs": "^4.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-remark": "^3.0.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-share": "^4.4.0",
    "styled-components": "^5.2.1",
    "styled-icons": "^10.29.0"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.4",
    "@types/jest": "^26.0.20",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "@types/webpack": "^4.41.26",
