{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "debug": false
      }
    ],
    "@babel/preset-typescript",
    [
      "babel-preset-gatsby-package",
      {
        "browser": true
      }
    ]
  ],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": true }]]
}
